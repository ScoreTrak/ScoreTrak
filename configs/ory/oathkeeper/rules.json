[
  {
    "id": "competition:read",
    "version": "v0.40.3",
    "upstream": {
      "url": "http://localhost:3001/"
    },
    "match": {
      "url": "http://localhost:4455/competitions/<.*>",
      "methods": ["GET"]
    },
    "authenticators": [{"handler": "anonymous"}],
    "authorizer": {
      "handler": "allow",
      "config": {
        "required_action": "read",
        "required_resource": "competition:$1",
        "subject": "{{ print .Subject }}"
      }
    },
    "mutators": [{"handler": "header"}],
    "errors": [{"handler": "json"}]
  },
